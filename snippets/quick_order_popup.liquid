<style>
  p.product_title_quick {
    font-family: Apercu Pro;
    font-size: 20px;
    line-height: 24px;
    color: #2e4433;
    margin: 0;
    margin-top: 8px;
    margin-bottom: 15px;
  }
  p.product_title_quick a:hover {
    color: #2e4433;
    text-decoration:none;
  }
  #category_shop_ass2_1 .margin_set_11 {
    margin-bottom:30px;
  }
  .quick_order_bthn_viewed {
    color: #f5f5f3;
    background: #2e4433;
    border-color: #2e4433;
    border-radius: 0px !important;
    font-family: 'Apercu';
  }
  .quick_order_bthn_viewed:hover {
    color: #f5f5f3;
  }
  .quick_order_bthn_viewed:focus {
    box-shadow : 0 0 0 0 rgb(25 118 210 / 25%);
  }
  .exampleModal_faded_0.exampleModal_faded__111 {
    width: 100vw;
    overflow-y: overlay;
    font-family: 'Apercu';
  }
  .product_quick_order_title {
    font-family: 'Apercu pro';
    font-size:24px;
  }
  .exampleModal_faded__111 a:hover ,.product_quick_order_title a:hover {
    text-decoration:none;
  }
  .product_title_quick.product_link_quick {
    TEXT-ALIGN: CENTER;
    WIDTH: 100%;
    TEXT-DECORATION: UNDERLINE;
  }
  .exampleModal_faded__111 .row.pt-0.zero_bot_pad_trade_shop_all {    
    width: 100%;
    margin: 0;
  }
  .exampleModal_faded_0 .modal-dialog {max-width: 1000px !important;}
  .exampleModal_faded_0 .modal-dialog .text-center {height: 100px;}
  .exampleModal_faded_0 .modal-dialog .text-center img {height: 100%;}
  .exampleModal_faded_0 .modal-dialog .rte.hide_desc_quick p {width: 100%;}
  .exampleModal_faded_0 .modal-dialog .product_quick_order_title {display: flex;font-size:20px;}
  .exampleModal_faded_0 .modal-dialog .product_quick_order_title a {width: 100%;     white-space: nowrap;}
  .exampleModal_faded_0 .modal-dialog .product_quick_order_title .product-price {width: 100%; display: flex; justify-content: flex-end;    font-size: 20px;}
  .exampleModal_faded_0 .modal-dialog .mx-n2.w-100 {display: flex; justify-content: center; align-items: center;}
  .exampleModal_faded_0 .modal-dialog .mx-n2.w-100 .custom-select {margin-bottom: initial !important;}
  @media screen and (max-width:500px) {
    .exampleModal_faded_0 img {
      height: 160px;
    }
    p.product_title_quick {
      font-size: 14px;
      margin-top: 0;
      margin-bottom: 5px;
    }
    .quick_order_bthn_viewed {
      font-size: 11px;
    }
    .exampleModal_faded__111 .modal-body {
      padding: 0;
    }
    .exampleModal_faded__111 .row.pt-0.zero_bot_pad_trade_shop_all {
      margin: 0;
      width: 100%;
    }
    .modal.exampleModal_faded_0.exampleModal_faded__111    {
      padding-right:0px !important;
    }
  }
  @media screen and (max-width: 850px) and (min-width: 576px)  {
    .modal-dialog {
      max-width: 80%;
    }
    .exampleModal_faded_0 img {
      height: 160px;
    }
    p.product_title_quick {
      font-size: 14px;
      margin-top: 0;
      margin-bottom: 5px;
    }
    .quick_order_bthn_viewed {
      font-size: 11px;
    }
  }
  @media screen and (max-width:370px) { 
    #category_shop_ass2_1 .margin_set_11 {
      width:100% !important;
    }
  }
  .rte.hide_desc_quick p,.rte.hide_desc_quick span {
    display:none;
  }
  .rte.hide_desc_quick p:first-child,.rte.hide_desc_quick span:first-child,.rte.hide_desc_quick {
    display:block;
    width: 460px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
  } 
  @media screen and (min-width:1600px) {
    .exampleModal_faded_0 .modal-dialog {
      max-width: 1090px;
    }
  }
  @media screen and (max-width:900px) and (min-width:650px)  {
    .exampleModal_faded_0 .modal-dialog {
      max-width: 660px !important;
    }  
    .rte.hide_desc_quick p:first-child, .rte.hide_desc_quick span:first-child, .rte.hide_desc_quick {
      width: 280px;
    }
    .bg-light.py-3.px-3.padding-0_rem {
      padding:0px !important;
      background-color: #fff !important;
    }
  }
  @media screen and (max-width:900px) {
    button.add-to-cart-btn.btn.class_for_del_rio_theme_btn.quick_order_bthn_viewed.navigable {
      font-size:12px;
    }  
    .exampleModal_faded_0 .modal-dialog .product_quick_order_title,.rte.hide_desc_quick p:first-child, .rte.hide_desc_quick span:first-child, .rte.hide_desc_quick {
      font-size:14px;
    }
    .rte.hide_desc_quick p:first-child, .rte.hide_desc_quick span:first-child, .rte.hide_desc_quick {
      width: 100%;
    }  
    .row.mx-n2.w-100.margin_0_rem {   
      margin-left: 0rem !important;
    }
  }
  @media screen and (max-width:500px) {  
    .padding_0_left {
      padding-left:0px !important;  
    }
    .exampleModal_faded_0 .modal-dialog .text-center .height_for_1 {
      height:54px;
    }
    .displa_flexr_0 {
      display:flex;
      -webkit-display:flex;
      align-items:center;
      justify-content: center;
    }
    .exampleModal_faded_0 .modal-dialog .product_quick_order_title .product-price {
    	font-size:15px;
    }
  }
    @media screen and (max-width:380px) {  
      .exampleModal_faded_0 .modal-dialog .product_quick_order_title, .rte.hide_desc_quick p:first-child, .rte.hide_desc_quick span:first-child, .rte.hide_desc_quick {
    	font-size:12px;
    }
    }
  .modal-title.exapmle_quick_order {
    width: 100%;
    text-align: center;
    font-family: 'Apercu Pro';
    padding-left: 20px;
   }
</style>  

<div class="modal fade exampleModal_faded_0 exampleModal_faded__111" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title exapmle_quick_order" id="exampleModalLabel">QUICK ORDER</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>     
      <div class="modal-body"> 
        <div class="pt-0  zero_bot_pad_trade_shop_all tabcontent_shop_all_tabs" id="category_shop_ass2_1">
          {% assign collectionallproducts = collections[section.settings.collection_handle].products  |  sort: 'price' | reverse %}
          {% for product in collectionallproducts %}
          {% if product.available %}
          <!-- Quick Order Sub Modal -->
          <div class="row mx-n2 w-100 margin_0_rem">
            <div class="padding_0_right col-lg-2 col-md-2 col-3">
              <div class="
                          displa_flexr_0
                          text-center
                          position-relative
                          overflow-hidden
                          mb-2
                          ">
                <img
                     class="product-featured-image img-fluid lazyload
                            {% for image in product.images %}
                            {% if image.aspect_ratio > 1.3 %}
                            height_for_1
                            {% endif %}
                            {% endfor %}

                            "    
                     src="{{ 'no-image.jpg' | asset_img_url: '400x400' }}"
                     data-src="{{ product.featured_image | img_url: '400x400' }}"   
                     alt="{{ product.featured_image.alt | escape }}"
                     loading="lazy"
                     >
                {% if product.compare_at_price > product.price %}
                <small class="product-discount-badge badge-right" style="{{ settings.color_on_sale | prepend: 'background-color: ' }}">
                  {{ product.compare_at_price | minus: product.price | times: 100.0 | divided_by: product.compare_at_price | money_without_currency | times: 100 | remove: '.0' | prepend: '-' | append: '%' }}
                </small>
                {% endif %}
              </div>
            </div>
            <div class="padding_0_left col-lg-6 col-md-6 col-9">
              <div>
                <h1 id="{{ product.id | prepend: 'quick-order-modal-label-' }}" class="mt-1 product_quick_order_title">
                  <a href="{{ product.url }}" class="text-dark">
                    {{ product.title | truncate:26 }}
                  </a>
                  <div class="product-price font-size-120" style="{{ settings.color_price | prepend: 'color: ' }}">
                    {% if product.compare_at_price_max > product.price %}
                    <span class="sr-only">{{ 'product.regular_price' | t }}</span>
                    {% if current_variant.compare_at_price > current_variant.price %}
                    <s class="text-muted opacity-75 mr-2">
                      {{ current_variant.compare_at_price | money }}
                    </s>
                    {% endif %}
                    {% endif %}
                    <span>
                      {{ product.price | money }}
                    </span>
                  </div>
                </h1>

                <div class="rte hide_desc_quick">
                  {{ product.description }}
                </div>
              </div>
            </div>
            <div class="col-lg-4 col-md-4">
              {%- assign current_variant = product.selected_or_first_available_variant -%}
              {% form 'product', product, data-productid: product.id, class: 'mb-3' %}
              <div class="bg-light py-3 px-3 padding-0_rem">
                <select name="id" data-productid="{{ product.id }}" id="{{ product.id | prepend: 'featured-collection-variant-' }}" class="custom-select mb-3 {% if product.has_only_default_variant %}d-none{% endif %}">
                  {% for variant in product.variants %}
                  <option 
                          {% unless variant.available %}disabled{% endunless %}
                          value="{{ variant.id }}"
                          >
                    {{ variant.title }} - {{ variant.price | money }}
                  </option>
                  {% endfor %}
                </select>
                <hr class="mt-4 mb-3 {% if product.has_only_default_variant %}d-none{% endif %}">
                <div class="d-flex justify-content-center">
                  <input type="number" class="form-control w-auto mr-2" style="max-width: 80px;" size="20" name="quantity" value="1" min="1" aria-label="{{ 'product.quantity' | t }}">
                  <button
                          class="add-to-cart-btn btn class_for_del_rio_theme_btn quick_order_bthn_viewed"
                          type="submit"
                          name="add"
                          data-loading-text="{{ 'misc.loading' | t }}"
                          data-add-to-cart-text="{{ 'product.add_to_cart' | t }}"
                          data-view-cart-text="{{ 'product.view_cart' | t }}"
                          aria-controls="cart-toast-wrapper"
                          >
                    <div class="d-none bs-spinner spinner-border spinner-border-sm mr-1" role="status" aria-hidden="true"></div>
                    <span>{{ 'product.add_to_cart' | t }}</span>
                  </button>
                </div>
              </div>
              {% endform %}                      
            </div>
          </div>	
          {% endif %}
          {% endfor %} 
          <p class="product_title_quick product_link_quick"><a href="/collections/all">VIEW ALL</a></p>
        </div>
      </div>   
    </div>     
  </div>
</div>



<script>
  $(document).ready(function(){
    $(".close_sub_modal",this).click(function(){
      var modal_close = $(this).parents(".modal_closed_custom").prev();
      modal_close.click();
    });
  });
</script>  

