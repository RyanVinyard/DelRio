}
.Whats-in-the-bottle_contact_us {
    padding-top: 21px;
}
.accordian_text_contact_us .main_contact_us .accordian_contact_us ul ul {
    font-size: 12.5px;
}


}

@media screen and (max-width: 991px) {

    .contact_us_page_section_1 {
