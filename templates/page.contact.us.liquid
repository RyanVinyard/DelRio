<style>
  
.contact_us_page_section_1 {
    background-color: #fcfcf5;
    padding: 170px 45px 90px;
}
h1.Wed-love-to-hear-fr_contact_us {
    font-family: 'Butler';
    font-size: 48px;
    font-weight: 300;
    font-stretch: normal;
    font-style: normal;
    line-height: 1.17;
    letter-spacing: normal;
    text-align: center;
    color: #171f15;
    margin-bottom: 70px;
  }
.contact_title_text_about_us p {
    margin-bottom: 0;
}
.contact_title_text_about_us {
    margin-bottom: 60px;
}
.contact_title_text_about_us:last-child {
    margin-bottom: 30px;
}

p.General-Questions_contact_us {
    font-family: Apercu Pro;
    font-size: 14px;
    font-weight: bold;
    font-stretch: normal;
    font-style: normal;
    line-height: 1.14;
    letter-spacing: 1px;
    text-align: center;
    color: #171f15;
    text-transform: uppercase;
    margin-bottom: 4px;
}
 p.hellodelriovineyard_contact_us {
    font-family: Apercu Pro;
    font-size: 32px;
    font-weight: bold;
    font-stretch: normal;
    font-style: normal;
    line-height: 1.25;
    letter-spacing: normal;
    text-align: center;
    color: #2e4433;
}

p.Like-Follow-Share_contact_us {
    font-family: Apercu Pro;
    font-size: 17px;
    font-weight: bold;
    font-stretch: normal;
    font-style: normal;
    line-height: 1.14;
    letter-spacing: 1px;
    text-align: center;
    color: #171f15;
    margin: 35px 0 20px;
}
  .social_icons_contact_us i {
        font-size: 24px;
        font-weight: normal;
        font-stretch: normal;
        font-style: normal;
        line-height: 1;
        letter-spacing: normal;
        color: #2e4433; 
}
.social_icons_contact_us {
    display: flex;
    justify-content: space-between;
    padding: 0px 5px;
}
.like_follow_share_contact_us {
    width: 16%;
    margin: 0 auto;
}

/* section 2 css */

h1.Frequently-Asked-Que_text_contact_us {
    font-family: 'Butler';
    font-size: 44px;
    font-weight: 300;
    font-stretch: normal;
    font-style: normal;
    line-height: 1.17;
    letter-spacing: normal;
    text-align: center;
    color: #171f15;
    margin: 0px;
}
  .contact_us_page_section_2 {
    background-color: #fcfcf5;
    padding: 80px 45px 100px;
}
p.All-Del-Rio-Wines-av_text_desc_contact_us {
    font-family: Apercu Pro;
    font-size: 18px;
    font-weight: bold;
    font-stretch: normal;
    font-style: normal;
    line-height: 1.6;
    letter-spacing: normal;
    text-align: center;
    color: #171f15;
    width: 74%;
    margin: 40px auto 60px;
}
hr.pixel_1_hr_contact_us {
    border: 0px;
    border-top:1px solid #bcc3bd;
    margin: 0;
}


/* accordian css */

.Whats-in-the-bottle_contact_us {
    padding-top: 12px;
}
.Whats-in-the-bottle_contact_us p {
    font-family: Apercu;
    font-size: 14px;
    font-weight: normal;
    font-stretch: normal;
    font-style: normal;
    line-height: 1.43;
    letter-spacing: normal;
    color: rgba(46, 68, 51, 0.87);
    cursor: pointer;
  }
  .Whats-in-the-bottle_contact_us a:hover {
      text-decoration: none;
  }
  .Whats-in-the-bottle_contact_us p:hover {
    color: rgba(46, 68, 51, 1);
    font-family: Apercu pro;
  }
  .maer_top_75_contact_us_sm {
    width: 75%;
    margin: 0 auto;
  }
  .contact_us_page_lady_img img {
      height: 200px;
  }



  .accordian_text_contact_us .accordian_contact_us h3 {
    font-family: Apercu Pro;
    font-size: 14.5px;
    font-weight: bold;
    font-stretch: normal;
    font-style: normal;
    line-height: 1.6;
    letter-spacing: normal;
    color: #171f15;
    cursor: pointer;
    margin: 0;
}
.accordian_text_contact_us .main_contact_us .accordian_contact_us ul ul {
    display: none;
    height: 100%;
    margin-bottom: -7px;
    font-family: Apercu;
    font-size: 14px;
    font-weight: normal;
    font-stretch: normal;
    font-style: normal;
    line-height: 1.43;
    letter-spacing: normal;
    color: #171f15;
    display: none;
    padding: 11px 0 0px;
    margin: 0;
}
.accordian_text_contact_us .main_contact_us .accordian_contact_us ul ul li p {
    opacity: 0;
    transition: all 0.5s;
    transform: translatey(-20px);
}
.accordian_text_contact_us .main_contact_us .accordian_contact_us ul ul.open p {
    opacity: 1;
    transform: translatey(0px);
}
.accordian_text_contact_us .main_contact_us .accordian_contact_us .arrow {
    display: inline-block;
    width: 9px;
    height: 9px;
    border-top: 2px solid #171f15;
    border-right: 2px solid #171f15;
    transform: rotate(135deg);
    transform-origin: center;
    float: right;
    margin-right: 8px;
    transition: transform 600ms cubic-bezier(0.175, 0.885, 0.32, 1);
}
.accordian_text_contact_us .main_contact_us .accordian_contact_us .arrow.closed {
    margin-top: 4px;
    transform: rotate(-45deg);
}
.accordian_text_contact_us .main_contact_us .accordion-item_contact_us {
    padding: 12px 0;
    margin: 0;
    overflow: hidden;
    border: none;
    outline: none;
}
.accordian_text_contact_us .main_contact_us .accordian_contact_us ul {
    padding: 0;
    margin: 0;
}
.Whats-in-the-bottle_contact_us p {
    font-family: Apercu;
    font-size: 14px;
    font-weight: normal;
    font-stretch: normal;
    font-style: normal;
    line-height: 1.43;
    letter-spacing: normal;
    color: rgba(46, 68, 51, 0.87);
    cursor: pointer;
    margin-bottom: 12px;
}
.accordian_text_contact_us .main_contact_us .accordian_contact_us {
    border-bottom: 1px solid #2e4433;
}
.accordian_text_contact_us .main_contact_us .accordian_contact_us:last-child {
    border-bottom: 0px;
}






