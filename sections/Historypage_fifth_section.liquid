<div class="padding-4_perk">
  <div class="row">
    <div class="col-lg-6 col-md-6 col-sm-12">
      <div class="perk_6_img">
        <div class="perk_7 perk_8">
          <img src="{{section.settings.his_pg_section_3_image_3  | img_url:'master'}}{{'history_page_imag_4.png' | asset_url }}" alt="">
          <p class="On-February-8-1865_history_2 desc_his_2">{{section.settings.his_pg_section_3_image_details_3}}</p>
        </div>
      </div>
    </div>
    <div class="col-lg-6 col-md-6 col-sm-12">
      <div class="perk_6_text perk_7_his">
        <div class="perk_vip_text">
          <h1 class="year_in_history_page">{{section.settings.his_pg_section_3_year_3}}</h1>
          <p class="On-February-8-1865_history_1">{{section.settings.his_pg_section_3_sub_heading_3}}</p>
          {% for block in section.blocks %}
          <p class="On-February-8-1865_history_2">{{block.settings.his_pg_section_3_Description_3}}</p>
         {% endfor %}
        </div>
      </div>
    </div>
  </div>
</div> 



{% schema %}
  {
    "name": "History Fifth section",
    "settings": [
     {
      "id": "his_pg_section_3_image_3",
      "type": "image_picker",
      "label": "image"
       },
     {
      "id": "his_pg_section_3_image_details_3",
      "type": "text",
      "label": "Image Details"
      },
     {
      "id": "his_pg_section_3_year_3",
      "type": "text",
      "label": "Year"
      },
     {
      "id": "his_pg_section_3_sub_heading_3",
      "type": "textarea",
      "label": "Sub Heading"
      }
],
  "blocks": [
    {
      "type": "select",
      "name": "Description",
      "settings": [
        {
          "id": "his_pg_section_3_Description_3",
          "type": "textarea",
       	  "label": "Description"
        }
      ]
    }
  ]
}
{% endschema %}

