        <div class="container main-contaainer margin-top">
          <div class="">
            <div class="row row-hattie">
                <div class="col-lg-6 col-md-12 col-sm-12 first-col">
                    <div class="row Schedule-a-Tour">
                    <h2>{{ section.settings.title }}</h2>
                </div>
                <div class="row Tours-are-available">
                        <p>{{ section.settings.text }}</p>
            </div>
            </div>
              {% for block in section.blocks %}
                <div class="col-lg-6 col-md-12 col-sm-12">
                  <div class="Meet-Hattie">
                    <img src="{{ block.settings.product-image | img_url: '394x503' }}">
                  </div>
                </div>
              {% endfor %}
                </div> 
          </div>
