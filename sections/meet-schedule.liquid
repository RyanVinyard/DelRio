{% schema %}
  {
    "name": "Schedule-a-Tour",
    "settings": [
    {
        "id": "title",
        "type": "text",
        "label": "Add custom text below"
      },
    {
        "id": "text",
        "type": "html",
        "label": "Add custom text below"
      }
  ],
"blocks": [
    {
      "type": "select",
      "name": "Add image",
      "settings": [
        {
          "id": "product",
          "type": "url",
       		"label": "add image"
        },
        {
          "id": "product-image",
          "type": "image_picker",
          "label": "image picker"
        }
      ]
    }
  ]
  }
{% endschema %}

